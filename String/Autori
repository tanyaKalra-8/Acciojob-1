import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner (System.in);
        String a = sc.next();
        String ret = "";
        ret+=a.charAt(0);
        for(int i=1; i<a.length();i++){
          if(a.charAt(i)=='-'){
            ret+=a.charAt(i+1);
          }
        }
        System.out.println(ret);
	}
}
