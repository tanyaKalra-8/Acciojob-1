import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		//your code here
      Scanner sc = new Scanner (System.in);
      String s = sc.next();
      int ans = -1;
      for(int i=s.length()-1;i>=0;i--){
        if(s.charAt(i)=='1'){
            ans= i;
            break;
        }
      }    
      System.out.println(ans); 
	}
}
