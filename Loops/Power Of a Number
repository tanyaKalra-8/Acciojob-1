import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner (System.in);
      int a = sc.nextInt();
      int b = sc.nextInt();
      int power =1;
      if(b>=1){
        for(int i=1;i<=b;i++){
          power=power*a;
        }
      }
      System.out.println(power);
	}
}
