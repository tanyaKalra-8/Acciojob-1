import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner (System.in);
        int n = sc.nextInt();
        int ans =0;
        for(int i=n ; i!=0; i=i/10){
          int r = i%10;
          ans= ans*10 + r;
        }
        System.out.println(ans);
	}
}
